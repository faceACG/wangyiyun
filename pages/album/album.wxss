.main {
  height: 100vh;
  width: 100vw;

}

.title {
  font-size: 15px;
  font-weight: 600;
  color: #333;
  margin: 10px 20px;
  display: block;
  padding-bottom: 10px;
  border-bottom: 3px solid #c02020;
}

.container {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.albumItem {
  height: 140px;
  width: 100px;
  margin-bottom: 10px;
  margin-right: 10px;
}

.pic {
  height: 200rpx;
  width: 200rpx;
}

.albumnmane{
  font-size: 13px;
  white-space: nowrap;
  overflow: hidden;
  display: block;
  width: 100%;
  text-overflow: ellipsis;
}